{
  "name": "reftex.com website",
  "scripts": {
    "postinstall": "elm-tooling install",
    "elm": "elm-pages dev",
    "cli": "elm-pages/generator/src/cli.js",
    "build": "tailwindcss -i input.css -o tailwind.css --minify && elm-pages build",
    "tail": "tailwindcss -i input.css -o tailwind.css",
    "dev": "concurrently \"elm-pages dev\" \"tailwindcss -i input.css -o tailwind.css --watch \"",
    "server": "python -m http.server 8421 --bind 127.0.0.12 --directory dist",
    "asdf": "aws s3 sync ./dist s3://reftex.com --delete"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.7",
    "autoprefixer": "^10.4.12",
    "concurrently": "7.2.2",
    "elm-codegen": "^0.2.0",
    "elm-optimize-level-2": "^0.3.5",
    "elm-tooling": "^1.8.0",
    "postcss": "^8.4.16",
    "tailwindcss": "^3.1.8",
    "vite-plugin-webfont-dl": "^3.1.1",
    "elm-pages": "^3.0.0-beta.7",
    "elm-review": "^2.7.6",
    "elm-tooling": "^1.7.0",
    "vite": "^3.1.8"
  },
  "dependencies": {
    "@netlify/functions": "^1.2.0"
  }
}
