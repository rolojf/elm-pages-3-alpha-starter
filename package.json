{
  "name": "mep3-app",
  "scripts": {
    "postinstall": "elm-tooling install",
    "elm": "elm-pages dev",
    "cli": "elm-pages/generator/src/cli.js",
    "build": "tailwindcss -i input.css -o tailwind.css --minify && elm-pages build",
    "tail": "tailwindcss -i input.css -o tailwind.css",
    "dev": "concurrently \"elm-pages dev\" \"tailwindcss -i input.css -o tailwind.css --watch \"",
    "server": "python -m http.server 8421 --bind 127.0.0.12 --directory dist",
    "asdf": "aws s3 sync ./dist s3://asdf.mx --delete"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "0.5.2",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@tailwindcss/typography": "^0.5.4",
    "autoprefixer": "^10.4.8",
    "concurrently": "7.2.2",
    "elm-optimize-level-2": "^0.3.5",
    "elm-pages": "file:elm-pages",
    "elm-review": "^2.7.4",
    "elm-tooling": "^1.8.0",
    "postcss": "^8.4.16",
    "tailwindcss": "^3.1.8",
    "vite": "^2.9.15"
  },
  "dependencies": {
    "@netlify/functions": "^1.2.0"
  }
}
